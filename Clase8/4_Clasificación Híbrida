// setup a feature collection for training data and extract sample data
var fc_test = ee.FeatureCollection( [ee.Feature(water_test, {'class':1} ),
                                        ee.Feature(urban_test, {'class':2} ),
                                        ee.Feature(forest_test, {'class':3} ),
                                        ee.Feature(snow_test, {'class':4} ),
                                        ee.Feature(meadow_test,{"class": 5}),
                                        ee.Feature(landslide_test,{"class": 6}),
                                        ee.Feature(bare_soil_test, {"class":7})])

// setup a feature collection for test data and extract sample data
var fc_training = ee.FeatureCollection( [ee.Feature(water, {'class':1} ),
                                        ee.Feature(urban, {'class':2} ),
                                        ee.Feature(forest, {'class':3} ),
                                        ee.Feature(snow, {'class':4} ),
                                        ee.Feature(meadow,{"class": 5}),
                                        ee.Feature(landslide,{"class": 6}),
                                        ee.Feature(bare_soil, {"class":7})])
                                        
Map.centerObject(fc_training, 14)

///////////////////////////////

////////// Cloud Mask

function maskS2clouds(image) {
  var qa = image.select('QA60');

  // Bits 10 and 11 are clouds and cirrus, respectively.
  var cloudBitMask = 1 << 10;
  var cirrusBitMask = 1 << 11;

  // Both flags should be set to zero, indicating clear conditions.
  var mask = qa.bitwiseAnd(cloudBitMask).eq(0)
      .and(qa.bitwiseAnd(cirrusBitMask).eq(0));

  return image.updateMask(mask);
}

var bands = ["B2", "B3", "B4", "B8", "B8A", "B11"]

var im_s2 = ic_s2.filterDate("2020-01-01", "2020-12-31")
                  .filterBounds(AOI)
                  .filterMetadata("CLOUDY_PIXEL_PERCENTAGE", "less_than", 10)
                  .map(maskS2clouds)
                  .median()
                  .select(bands)
                  .multiply(0.0001)
                  .clip(AOI)
                  
var s2_col = ic_s2.filterDate("2020-01-01", "2020-12-31")
                  .filterBounds(AOI)
                  .filterMetadata("CLOUDY_PIXEL_PERCENTAGE", "less_than", 10)
                  .map(maskS2clouds)

//////////////// Obtener NDVI's

/////// NDVImax, NDVImin, NDVImean, NDVIstdDev

function NDVI (img){
  
  var ndvi = img.normalizedDifference(["B8", "B4"]).rename("NDVI")
  
  return ndvi
  
}

var NDVIcol= s2_col.map(NDVI)

var NDVImean = NDVIcol.mean().rename("NDVImean")

var NDVImax = NDVIcol.max().rename("NDVImax")

var NDVImin = NDVIcol.min().rename("NDVImin")

var NDVIstdDev = NDVIcol.reduce(ee.Reducer.stdDev())

////// DEM: Slope.

var elevacion = DEM.select("elevation").divide(10000)

var slope = ee.Terrain.slope(elevacion).rename("slope").divide(100)

///////////// No supervisada

var no_supervisada = wekaKMeans.divide(100).rename("clusters")

////////////////////////////////////////////// Imagen a Classificar ///////////////////////////////////////////////

var imagen = im_s2.addBands(NDVImax).addBands(NDVImin).addBands(no_supervisada).addBands(NDVImean).addBands(NDVIstdDev).addBands(slope).addBands(elevacion)


Map.addLayer(imagen, {min:0, max:0.35, bands:["B4", "B3", "B2"]}, "RGB a clasificar")

///////////////////////////////////////////////// CLASIFICACION SUPERVISADA ///////////////////////////////////////////////////////////

var training = imagen.sampleRegions({
  
  collection: fc_training,
  properties: ["class"],
  scale:10

})

print(training.limit(10))

////////////////// Clasificacion

var classifier = ee.Classifier.smileRandomForest(25).train(training, "class")

var classified = imagen.classify(classifier)

Map.addLayer(classified.clip(AOI), {min:1, max:7,palette:["blue", "red", "green", "white","#1bff42","#ffdc5e", "yellow"]}, "RF Clasificacion")

/////////////////////////////////////////////// EVALUACION DE LA FIABILIDAD

var test = classified.sampleRegions({
  
  collection: fc_test,
  properties: ["class"],
  scale:10

})

print(test.size())

/////////////////// Fiabilidad

var errorMatrix = test.errorMatrix("class", "classification")

var OA = errorMatrix.accuracy()

var kappa = errorMatrix.kappa()


var sensitivity = errorMatrix.producersAccuracy()

print(errorMatrix, "Error Matrix")
print(OA, "Overall Accuracy")
print(kappa, "Kappa Index")
print(sensitivity, "Sensitivity")

