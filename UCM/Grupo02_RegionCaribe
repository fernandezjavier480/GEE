//*******************************************************************************************
//                      1. MOSAICO DE IMÁGENES PARA LA REGIÓN CARIBE -    

/* Definición de la región Caribe Colombiana conformada por los departamentos de Atlántico,
  Bolivar, Cesar, Cordoba, La Guajira, Magdalena, Antioquia, Sucre y Chocó (Acandí y Unguía) 
  a partir de la Capa de Unidades Administrativas Globales
  (GAUL), la cual compila las unidades administrativas para todos los países del mundo. 
  El GAUL siempre mantiene capas globales con un sistema de codificación unificado, primero 
  (Nivel 0: Países, Nivel:1 Departamentos o estados y Nivel 2: Municipios o distritos).
  Reemplace las variables en rojo de país, región y municipio: */
   
   
// Cargar en una variable la colección de límites administrativos nivel 1 (departamentos)

//*******************************************************************************************
//                      1. MOSAICO DE IMÁGENES PARA LA REGIÓN CARIBE -    
/* Definición de la región Caribe Colombiana conformada por los departamentos de Atlántico,
  Bolivar, Cesar, Cordoba, La Guajira, Magdalena, Antioquia, Sucre y Chocó (Acandí y Unguía) 
  a partir de la Capa de Unidades Administrativas Globales
  (GAUL), la cual compila las unidades administrativas para todos los países del mundo. 
  El GAUL siempre mantiene capas globales con un sistema de codificación unificado, primero 
  (Nivel 0: Países, Nivel:1 Departamentos o estados y Nivel 2: Municipios o distritos).
  Reemplace las variables en rojo de país, región y municipio: */
// Cargar en una variable la colección de límites administrativos nivel 1 (departamentos)
var limites = limites2;
// Definir una variable que contenga el nombre del país de interés nivel 0
var pais = 'Colombia';
// Definir una variable que contenga el nombre del departamento de interés nivel 1

//DEPARTAMENTO 1-ATLANTICO
var departamento = 'Atlantico';
// Crear una variable con la colección filtrada de limites administrativos filtrada por país y departamento
var AOI= limites.filter(ee.Filter.eq('ADM0_NAME', pais))
                .filter(ee.Filter.eq('ADM1_NAME', departamento));
var vacia2 = ee.Image().byte();  
var linea2 = vacia2.paint({
  'featureCollection': AOI,
  'color': 1,
  'width': 1.5
}); 
//Mostrar en la consola la colección filtrada a límite de interés
print (AOI, 'Área de Interés: ' + departamento + ' ' + pais);
Map.addLayer(AOI,false);

//DEPARTAMENTO 2-BOLIVAR
var departamento2 = 'Bolivar';
// Crear una variable con la colección filtrada de limites administrativos filtrada por país y departamento
var AOI2= limites.filter(ee.Filter.eq('ADM0_NAME', pais))
                .filter(ee.Filter.eq('ADM1_NAME', departamento2));
Map.addLayer(AOI2,false);
var vacia2 = ee.Image().byte();  
var linea2 = vacia2.paint({
  'featureCollection': AOI2,
  'color': 1,
  'width': 1.5
});
// Imprimir el vector del AOI2, puede traer el nombre de las variables ya definidas anteriormente:
Map.addLayer(linea2, {'palette': '0204ff'}, 'Área de Interés: ' + departamento2 + ' ' + pais,false);

//DEPARTAMENTO 3-CESAR
var departamento3 = 'Cesar';
// Crear una variable con la colección filtrada de limites administrativos filtrada por país y departamento
var AOI3= limites.filter(ee.Filter.eq('ADM0_NAME', pais))
                .filter(ee.Filter.eq('ADM1_NAME', departamento3));
Map.addLayer(AOI3,false);
var vacia2 = ee.Image().byte();  
var linea2 = vacia2.paint({
  'featureCollection': AOI3,
  'color': 1,
  'width': 1.5
});
// Imprimir el vector del AOI3, puede traer el nombre de las variables ya definidas anteriormente:
Map.addLayer(linea2, {'palette': '0204ff'}, 'Área de Interés: ' + departamento3 + ' ' + pais,false);

//DEPARTAMENTO 4-CORDOBA
var departamento4 = 'Cordoba';
// Crear una variable con la colección filtrada de limites administrativos filtrada por país y departamento
var AOI4= limites.filter(ee.Filter.eq('ADM0_NAME', pais))
                .filter(ee.Filter.eq('ADM1_NAME', departamento4));
Map.addLayer(AOI4,false);
var vacia2 = ee.Image().byte();  
var linea2 = vacia2.paint({
  'featureCollection': AOI4,
  'color': 1,
  'width': 1.5
});
// Imprimir el vector del AOI4, puede traer el nombre de las variables ya definidas anteriormente:
Map.addLayer(linea2, {'palette': '0204ff'}, 'Área de Interés: ' + departamento4 + ' ' + pais,false);

//DEPARTAMENTO 5-LA GUAJIRA
var departamento5 = 'Guajira';
// Crear una variable con la colección filtrada de limites administrativos filtrada por país y departamento
var AOI5= limites.filter(ee.Filter.eq('ADM0_NAME', pais))
                .filter(ee.Filter.eq('ADM1_NAME', departamento5));
Map.addLayer(AOI5,false);
var vacia2 = ee.Image().byte();  
var linea2 = vacia2.paint({
  'featureCollection': AOI5,
  'color': 1,
  'width': 1.5
});
// Imprimir el vector del AOI5, puede traer el nombre de las variables ya definidas anteriormente:
Map.addLayer(linea2, {'palette': '0204ff'}, 'Área de Interés: ' + departamento5 + ' ' + pais,false);

//DEPARTAMENTO 6-MAGDALENA
var departamento6 = 'Magdalena';
// Crear una variable con la colección filtrada de limites administrativos filtrada por país y departamento
var AOI6= limites.filter(ee.Filter.eq('ADM0_NAME', pais))
                .filter(ee.Filter.eq('ADM1_NAME', departamento6));
Map.addLayer(AOI6,false);
var vacia2 = ee.Image().byte();  
var linea2 = vacia2.paint({
  'featureCollection': AOI6,
  'color': 1,
  'width': 1.5
});
// Imprimir el vector del AOI6, puede traer el nombre de las variables ya definidas anteriormente:
Map.addLayer(linea2, {'palette': '0204ff'}, 'Área de Interés: ' + departamento6 + ' ' + pais,false);

//DEPARTAMENTO 7-SUCRE
var departamento7 = 'Sucre';
// Crear una variable con la colección filtrada de limites administrativos filtrada por país y departamento
var AOI7= limites.filter(ee.Filter.eq('ADM0_NAME', pais))
                .filter(ee.Filter.eq('ADM1_NAME', departamento7));
Map.addLayer(AOI7,false);
var vacia2 = ee.Image().byte();  
var linea2 = vacia2.paint({
  'featureCollection': AOI7,
  'color': 1,
  'width': 1.5
});
// Imprimir el vector del AOI7, puede traer el nombre de las variables ya definidas anteriormente:
Map.addLayer(linea2, {'palette': '0204ff'}, 'Área de Interés: ' + departamento7 + ' ' + pais,false);

//DEPARTAMENTO 8-ANTIOQUIA
var departamento8 = 'Antioquia';
// Crear una variable con la colección filtrada de limites administrativos filtrada por país y departamento
var AOI8= limites.filter(ee.Filter.eq('ADM0_NAME', pais))
                .filter(ee.Filter.eq('ADM1_NAME', departamento8));
Map.addLayer(AOI8,false);
var vacia2 = ee.Image().byte();  
var linea2 = vacia2.paint({
  'featureCollection': AOI8,
  'color': 1,
  'width': 1.5
});
// Imprimir el vector del AOI8, puede traer el nombre de las variables ya definidas anteriormente:
Map.addLayer(linea2, {'palette': '0204ff'}, 'Área de Interés: ' + departamento8 + ' ' + pais,false);

//Selección de los municipios del Chocó incluidos dentro de la región Caribe

var departamento9 = 'Choco';
//Municipio de Acandí
var municipio1 = 'Acandi';

// Crear una variable con la colección filtrada de limites administrativos filtrada por país y departamento
var AOI9= limites.filter(ee.Filter.eq('ADM0_NAME', pais))
                .filter(ee.Filter.eq('ADM1_NAME', departamento9))
                .filter(ee.Filter.eq('ADM2_NAME', municipio1));

Map.addLayer(AOI9,false);

var vacia2 = ee.Image().byte();  
var linea2 = vacia2.paint({
  'featureCollection': AOI9,
  'color': 1,
  'width': 1.5
});

// Imprimir el vector del AOI9, puede traer el nombre de las variables ya definidas anteriormente:
Map.addLayer(linea2, {'palette': '0204ff'}, 'Área de Interés:' + municipio1 + ' ' + departamento9 + ' ' + pais,false);

//Municipio Unguia
var departamento10 = 'Choco';
var municipio2 = 'Unguia';

// Crear una variable con la colección filtrada de limites administrativos filtrada por país y departamento
var AOI10= limites.filter(ee.Filter.eq('ADM0_NAME', pais))
                .filter(ee.Filter.eq('ADM1_NAME', departamento10))
                .filter(ee.Filter.eq('ADM2_NAME', municipio2));

Map.addLayer(AOI10,false);

var vacia2 = ee.Image().byte();  
var linea2 = vacia2.paint({
  'featureCollection': AOI10,
  'color': 1,
  'width': 1.5
});

// Imprimir el vector del AOI10, puede traer el nombre de las variables ya definidas anteriormente:
Map.addLayer(linea2, {'palette': '0204ff'}, 'Área de Interés:' + municipio2 + ' ' + departamento10 + ' ' + pais,false);

// Fusionamos los dos vectores, pero aun conservando sus geometrías por separado:
var conbineAOI = AOI.merge(AOI2)
                    .merge(AOI3)
                    .merge(AOI4)                
                    .merge(AOI5)
                    .merge(AOI6)
                    .merge(AOI7)
                    .merge(AOI8)
                    .merge(AOI9)
                    .merge(AOI10);
                 
print(conbineAOI,'combinacion')

// Obtenemos la unión de las geometrías AOI en un solo polígono conectado:
var countyConnectDiss=conbineAOI.union();

// Creamos una capa vacía y adicionamos el vector.
var vacia3 = ee.Image().byte();  
var linea3 = vacia3.paint({
  'featureCollection': countyConnectDiss,
  'color': 1,
  'width': 2.5
});

// Imprimir el vector final AOI, que contendrá las capas disueltas anteriormente:
Map.addLayer(linea3, {'palette': 'red'}, 'conbinacion'+ municipio2 + ''+ municipio1 + ''+ departamento8 + ''+ departamento7 + ''+ departamento6 + ''+ departamento5 + ''+ departamento4 + ''+ departamento3 + ' '+ departamento2 + ' ' + departamento + ' ' + pais);

// Centrar el mapa en la zona de interés
//Map.centerObject(countyConnectDiss);

var imageCollection=ee.ImageCollection("LANDSAT/LC08/C01/T1_TOA")
      .filterDate('2018-01-01','2019-12-31')
      .filterBounds(countyConnectDiss)
      .filterMetadata("CLOUD_COVER","less_than",30);

var image=imageCollection.mosaic();

Map.addLayer(image.clip(countyConnectDiss),{bands:["B6","B5","B4"]},"Mosaico de"+countyConnectDiss);
Map.addLayer(image.clip(countyConnectDiss),{bands:["B5","B4","B3"]},"Mosaico de"+countyConnectDiss);
Map.addLayer(image.clip(countyConnectDiss),{bands:["B5","B6","B4"]},"Mosaico de"+countyConnectDiss);


Map.centerObject(countyConnectDiss);