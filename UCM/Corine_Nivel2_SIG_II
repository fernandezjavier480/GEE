    /* color: #d63000 */
    /* displayProperties: [
      {
        "type": "rectangle"
      }
    ] */
    ee.Geometry.Polygon(
        [[[-75.87455491461556, 6.8108857727596614],
          [-75.87455491461556, 6.353867159258857],
          [-75.44746140875618, 6.353867159258857],
          [-75.44746140875618, 6.8108857727596614]]], null, false),
    table = ee.FeatureCollection("users/nico170992/Municipio_SIGII");
/***** End of imports. If edited, may not auto-convert in the playground. *****/
var shp=ee.FeatureCollection(polygon)
//Visualizacin Limites
var RellenoLimite= ee.Image().byte();
var Limite=RellenoLimite.paint({featureCollection:polygon,width:2});
Map.addLayer(Limite, {palette:'red'}, 'Municipios Interes')
Map.centerObject(polygon,7)
Map.addLayer(table, {palette:'blue'}, 'Municipios')

//Coleccin a utilizar 
var landsatCollection = ee.ImageCollection('LANDSAT/LC08/C01/T1')
    .filterDate('2020-01-01', '2020-12-31');

// Composicin libre de Nubes
var composite = ee.Algorithms.Landsat.simpleComposite({
  collection: landsatCollection,
  asFloat: true
});

// Visualizacin de la composicin 
Map.addLayer(composite, {bandas: ['B4', 'B3', 'B2'], max: 0.5, gamma: 2}, 'L8 Image', false);
Map.addLayer(composite.clip(shp),{bands:["B5","B6","B4"]},'Mosaico L8 Image',false);


// Unin de las muestras recolectadas dentro de una misma coleccin
var muestras = Zonas_Urbanizadas.merge(Zonas_Extraccion_Minera).merge(Zonas_Verdes_Artificializadas)
               .merge(Cultivos_permanentes).merge(Pastos).merge(Bosques).merge(Vegetacion_Herbacea_Arbustiva)
               .merge(Areas_Abiertas_Sin_Vegetacion).merge(Areas_Humedas_Continentales).merge(Aguas_Continentales);
print(muestras, 'muestras')

// seleccin de bandas para el entrenamiento
var bandas = ['B2', 'B3', 'B4', 'B5', 'B6', 'B7'];

// Muestreo en la imagen de entrada para obtener un FeatureCollection.
var entrenamiento = composite.select(bandas).sampleRegions({
  collection: muestras, 
  properties: ['corineland'], 
  scale: 30
});

// Clasificacin supervisada con Random Forest 
// Hacer la clasificacin con el algoritmo de Random Forest
var clasificador = ee.Classifier.smileRandomForest(100).train({
  features: entrenamiento, 
  classProperty: 'corineland', 
  inputProperties: bandas
});

// Clasificar la imagen de entrada
var clasificada = composite.select(bandas).classify(clasificador);

// Definir la paleta de colores para la clasificacin
var palette = [
  '#dddbe4', // Zonas_Urbanizadas (11) // 
  '#85827d', // Zonas_Extraccion_Minera (13) // 
  '#93fa06', // Zonas_Verdes_Artificializadas (14) // 
  '#ffc82d', // Cultivos_permanentes (22) // 
  '#f9fd03', // Pastos (23) // 
  '#19701a', // Bosques (31) // 
  '#aaff64', // Vegetacion_Herbacea_Arbustiva (32) // 
  '#c8b45f', // Areas_Abiertas_Sin_Vegetacion (33) // 
  '#1bfffa', // Areas_Humedas_Continentales (41) // 
  '#061ef7'  // Aguas_Continentales (51) // 
];

// Mapeo de la clasificacin supervisada 
Map.addLayer(clasificada, {min: 11, max: 51, palette: palette}, 'Imagen Clasificada',false);
//Map.addLayer(clasificada.clip(shp),{min: 11, max: 51, palette: palette},'Imagen Clasificada Municipios');
//Matriz de confusin y presicin de la clasificacin 
print('Matriz de error: ', clasificador.confusionMatrix());
print('Exactitud: ', clasificador.confusionMatrix().accuracy());

//Generacin de link de descarga
var URLDescarga=clasificada.clip(polygon).getDownloadURL({
  scale:50,
  region:polygon});
  print(URLDescarga);
